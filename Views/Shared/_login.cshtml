<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MedBox Smart</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/login.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/CaixaINteligenteWeb.styles.css" asp-append-version="true" />
</head>
<body>
    
        <main>
            <!-- Conteúdo específico da página será inserido aqui -->
            @RenderBody()
        </main>
   

    <footer class="border-top footer text-muted" background-color: transparent; color: white; position: fixed; bottom: 0; width: 100%;">
        <div style="text-align: center; padding: 10px 0; color: white;" class="container">
            &copy; 2023 - MedBox Smart
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
